{
    "name": "serwersms/serwersms-php-client",
    "version": "1.0.0",
    "description": "SerwerSMS API PHP",
    "keywords": ["serwersms", "php", "api"],
    "homepage": "http://github.com/SerwerSMSpl/serwersms-php-api-v2",
    "type": "library",
    "require": {
        "php": ">=5.3.0",
        "lib-curl": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*"
    },
    "autoload": {
        "psr-4": { "SerwerSMS\\": 
                    [
                        "src/",
                        "src/SerwerSMS"
                    ] }
    },
    "autoload-dev": {
        "psr-4": { "SerwerSMS\\Tests\\": "tests/" }
    }
}